{
    "extends": "tslint:all",
    "rules": {
        "quotemark": false,
        "semicolon": false,
        "linebreak-style": [true, "CRLF"],
        "only-arrow-functions": [true, "allow-declarations"],
        "no-inferrable-types": false,
        "newline-before-return": false,
        "no-default-export": false,
        "max-line-length": false
    }
}